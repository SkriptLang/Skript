test "sorted indices on map":
	set {_test::1} to 111
	set {_test::2} to 555
	set {_test::3} to 444
	set {_test::1::a} to 2
	set {_test::2::b} to 3
	set {_test::3::c} to 6
	set {_indices::*} to (sorted indices of {_test::*} in ascending order)
	assert {_indices::*} is ("1", "3", "2") with "sorted indices on list with children threw or was incorrect"
